class Candy {
  private String candyColor;
  private int special;
  private int score;
  
  public String getColor() {
    return candyColor;
  }
  
  public int getScore() {
    return score;
  }
  
  public int getSpecial() {
    return special;
  }
  
  public void setSpecial(int s) {
    special = s;
  }
  
  public void setScore(int s) {
    score = s;
  }
  
}
